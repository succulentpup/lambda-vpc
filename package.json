{
  "name": "lambda-vpc",
  "version": "1.0.0",
  "description": "Lambda inside a vpc accessing S3, SQS, secrets manager etc.",
  "author": "Ganesh Adapa",
  "license": "MIT",
  "scripts": {
    "cm": "git-cz",
    "check:updates": "npx npm-check-updates -u",
    "lint": "eslint 'src/*.{js,ts}'",
    "type-check": "tsc --noEmit",
    "all-static-checks": "yarn lint && yarn type-check",
    "serverless": "serverless",
    "sls": "sls",
    "deploy:dev": "yarn all-static-checks && sls deploy --stage dev",
    "deploy:function": "yarn all-static-checks && sls deploy -f"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.89",
    "@types/node": "^17.0.9",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "git-cz": "^4.8.0",
    "prettier": "^2.5.1",
    "serverless": "^3.3.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.3.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@dazn/lambda-powertools-logger": "^1.28.1",
    "@middy/core": "^2.5.5",
    "@middy/http-cors": "^2.5.5",
    "@middy/http-event-normalizer": "^2.5.5",
    "@middy/http-header-normalizer": "^2.5.5",
    "@middy/http-json-body-parser": "^2.5.5",
    "@middy/http-security-headers": "^2.5.5",
    "@middy/validator": "^2.5.5",
    "aws-lambda": "^1.0.7",
    "axios": "^0.24.0",
    "global": "^4.4.0",
    "statuses": "^2.0.1"
  }
}
